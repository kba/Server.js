    "mongoose": "^4.1.10",
    "mongoose-jsonld": "^0.1.20",
    "n3": "^0.4.0",
    "negotiate": "^1.0.1",
    "qejs": "^3.0.0",
    "request": "^2.27.0",
    "tson": "0.0.5",
    "uritemplate": "^0.3.0"
  },
  "optionalDependencies": {
    "hdt": "^1.1.0",
    "access-log": "^0.3.9"
  },
  "devDependencies": {
    "chai": "^1.9.0",
    "jshint": "^2.1.10",
    "mocha": "^1.17.1",
    "pre-commit": "^0.0.9",
    "sinon": "1.10.3",
    "sinon-chai": "^2.6.0",
    "supertest": "^0.13.0"
  },
